{
  "compilerOptions": {
    "target": "es2017",
    "allowJs": true,
    "moduleResolution": "node",
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": false,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noEmitOnError": false,
    "noEmit": true,
    "sourceMap": true,
    "baseUrl": ".",
    "module": "es6",
    "paths": {
      "redux": ["node_modules/redux/index.d.ts"],
      "ember-redux": ["node_modules/ember-redux/index.d.ts"],
      "seamless-immutable": ["node_modules/seamless-immutable-tsdefs/index.d.ts"],
      "fetch": ["node_modules/ember-fetch/index.d.ts"],
      "reselect": ["node_modules/reselect/lib/index.d.ts"],
      "statk/*": [
        "app/*"
      ],
      "*": [
        "types/*"
      ]
    }
  },
  "include": [
    "app",
    "types"
  ]
}
